<Window x:Class="Koi88_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Koi88_WPF"
        mc:Ignorable="d"
        Title="Koi88" Height="570" Width="800" WindowStartupLocation="CenterScreen"
        >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition Height="500"/>
        </Grid.RowDefinitions>

        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden"/>
        <TextBlock Name="TextBlockWelcome" Grid.Row="0" HorizontalAlignment="Left" Margin="130,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Height="50" Width="145"/>
        <Button Grid.Row="0" Name="ButtonLogout" Content="Logout" HorizontalAlignment="Left" Margin="613,0,0,0" VerticalAlignment="Center" Height="50" Width="95" Click="ButtonLogout_OnClick_Click"/>

    </Grid>
</Window>
